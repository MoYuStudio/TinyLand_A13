script/source = "
extends Camera3D

var rand_int = randi_range(1,3)

func _ready():
	pass

func _process(delta):
	if rand_int == 1:
		rotation_degrees.x += 0.01
	elif rand_int == 2:
		rotation_degrees.y += 0.01
	elif rand_int == 3:
		rotation_degrees.z += 0.01
	
"

