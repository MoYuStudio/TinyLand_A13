# Get the gravity from the project settings to be synced with RigidBody nodes.
var 重力 = ProjectSettings.get_setting('physics/3d/default_gravity')

func _physics_process(delta):
	if not is_on_floor():
		velocity.y -= 重力 * delta

	velocity.x = 移动速度
	# velocity.z = 1
	move_and_slide()
